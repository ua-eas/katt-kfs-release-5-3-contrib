		<parameter id="className" value="org.jetel.graph.dictionary.ReadableChannelDictionaryType"/>
	</extension>
	
	<extension point-id="dictionaryType">
		<parameter id="type" value="writable.channel"/>
		<parameter id="className" value="org.jetel.graph.dictionary.WritableChannelDictionaryType"/>
	</extension>
	
	<extension point-id="tlCompiler">
		<parameter id="type" value="simple.compiler"/>
		<parameter id="className" value="org.jetel.ctl.TLCompiler"/>
	</extension>
	
</plugin>
