		<parameter id="className" value="org.jetel.graph.dictionary.StringDictionaryType"/>
	</extension>
	
	<extension point-id="dictionaryType">
		<parameter id="type" value="object"/>
		<parameter id="className" value="org.jetel.graph.dictionary.ObjectDictionaryType"/>
	</extension>

	<extension point-id="dictionaryType">
