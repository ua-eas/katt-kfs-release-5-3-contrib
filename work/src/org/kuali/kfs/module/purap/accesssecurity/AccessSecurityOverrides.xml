<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">    
    
    <bean id="PaymentRequestDocument-sourceAccountingLineGroup" parent="PaymentRequestDocument-sourceAccountingLineGroup-parentBean" scope="prototype">
        <property name="accountingLineGroupClass" value="org.kuali.kfs.sec.document.web.CollectionSecAccountingLineGroupImpl" />
    </bean>
    
    <bean id="PurchaseOrderDocument-sourceAccountingLineGroup" parent="PurchaseOrderDocument-sourceAccountingLineGroup-parentBean" scope="prototype">
        <property name="accountingLineGroupClass" value="org.kuali.kfs.sec.document.web.CollectionSecAccountingLineGroupImpl" />
    </bean>
    
    <bean id="PurchaseOrderDocument-distributeSourceAccountingLineGroup" parent="PurchaseOrderDocument-distributeSourceAccountingLineGroup-parentBean" scope="prototype">
        <property name="accountingLineGroupClass" value="org.kuali.kfs.sec.document.web.CollectionSecAccountingLineGroupImpl" />
    </bean>
    
     <bean id="RequisitionDocument-sourceAccountingLineGroup" parent="RequisitionDocument-sourceAccountingLineGroup-parentBean" scope="prototype">
         <property name="accountingLineGroupClass" value="org.kuali.kfs.sec.document.web.CollectionSecAccountingLineGroupImpl" />
     </bean>
     
     <bean id="RequisitionDocument-distributeSourceAccountingLineGroup" parent="RequisitionDocument-distributeSourceAccountingLineGroup-parentBean" scope="prototype">
         <property name="accountingLineGroupClass" value="org.kuali.kfs.sec.document.web.CollectionSecAccountingLineGroupImpl" />
     </bean>
     
     <bean id="VendorCreditMemoDocument-sourceAccountingLineGroup" parent="VendorCreditMemoDocument-sourceAccountingLineGroup-parentBean" scope="prototype">
         <property name="accountingLineGroupClass" value="org.kuali.kfs.sec.document.web.CollectionSecAccountingLineGroupImpl" />
     </bean>
    
</beans>    
