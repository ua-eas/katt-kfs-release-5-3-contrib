  <bean id="PurchaseOrderRetransmitDocument" parent="PurchaseOrderRetransmitDocument-parentBean"/>

  <bean id="PurchaseOrderRetransmitDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="documentTypeName" value="PORT"/>
    <property name="sessionDocument" value="true"/>
    <property name="documentClass" value="org.kuali.kfs.module.purap.document.PurchaseOrderRetransmitDocument"/>
    <property name="baseDocumentClass" value="org.kuali.kfs.module.purap.document.PurchaseOrderRetransmitDocument"/>
    <property name="businessRulesClass" value="org.kuali.rice.krad.rules.TransactionalDocumentRuleBase"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.module.purap.document.authorization.PurchasingAccountsPayableTransactionalDocumentAuthorizerBase"/>
    <property name="documentPresentationControllerClass" value="org.kuali.kfs.module.purap.document.authorization.PurchaseOrderRetransmitDocumentPresentationController"/>
    <property name="importedLineParserClass" value="org.kuali.kfs.module.purap.businessobject.PurApAccountingLineParser" />
    <property name="potentiallySensitive" value="true"/>
    <property name="workflowAttributes">
    	<ref bean="PurchaseOrderRetransmitDocument-workflowAttributes"/>
    </property>
    <property name="accountingLineGroups">
    	<map>
    		<entry>
    			<key><value>source</value></key>
	    		<ref bean="PurchaseOrderDocument-sourceAccountingLineGroup" parent="AccountingLineGroup"/>
	    	</entry>
    	</map>
    </property>    
	<property name="relationships">
    	<list>
			<bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="deliveryCampus"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="deliveryCampusCode" p:targetName="campusCode"/>
                </list>
              </property>
    		</bean>
    	</list>
    </property>
  </bean>

    <!-- workflow attributes -->
    <bean id="PurchaseOrderRetransmitDocument-workflowAttributes" parent="PurchaseOrderRetransmitDocument-workflowAttributes-parentBean"/>
  
    <bean id="PurchaseOrderRetransmitDocument-workflowAttributes-parentBean" class="org.kuali.rice.krad.datadictionary.WorkflowAttributes" abstract="true">
    	<property name="searchingTypeDefinitions">
    		<ref bean="SearchingTypeDefinitions-PurchaseOrderDocumentFamily"/>
    	</property>
    </bean>
      

</beans>
