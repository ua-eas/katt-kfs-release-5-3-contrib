  <bean id="CapitalAssetSystemTypeMaintenanceDocument" parent="CapitalAssetSystemTypeMaintenanceDocument-parentBean"/>

  <bean id="CapitalAssetSystemTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.CapitalAssetSystemType"/>
    <property name="maintainableClass" value="org.kuali.kfs.sys.document.FinancialSystemMaintainable"/>
    <property name="allowsNewOrCopy" value="false"/>
    <property name="maintainableSections">
      <list>
        <ref bean="CapitalAssetSystemTypeMaintenanceDocument-EditCapitalAssetSystemType"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>capitalAssetSystemTypeCode</value>
      </list>
    </property>
    
    
    
    <property name="documentTypeName" value="PMSY"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
    	<ref bean="CapitalAssetSystemTypeMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="CapitalAssetSystemTypeMaintenanceDocument-EditCapitalAssetSystemType" parent="CapitalAssetSystemTypeMaintenanceDocument-EditCapitalAssetSystemType-parentBean"/>

  <bean id="CapitalAssetSystemTypeMaintenanceDocument-EditCapitalAssetSystemType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Capital Asset System Type"/>
    <property name="title" value="Edit Capital Asset System Type"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="capitalAssetSystemTypeCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="capitalAssetSystemTypeDescription" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="active" p:defaultValue="true"/>
      </list>
    </property>
  </bean>
  
  <!-- workflow attributes -->
  <bean id="CapitalAssetSystemTypeMaintenanceDocument-workflowAttributes" parent="CapitalAssetSystemTypeMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="CapitalAssetSystemTypeMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.krad.datadictionary.WorkflowAttributes" abstract="true"/>
</beans>
