  <bean id="CustomerAddressTypeMaintenanceDocument" parent="CustomerAddressTypeMaintenanceDocument-parentBean"/>

  <bean id="CustomerAddressTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.CustomerAddressType"/>
    <property name="maintainableClass" value="org.kuali.kfs.sys.document.FinancialSystemMaintainable"/>
    <property name="maintainableSections">
      <list>
        <ref bean="CustomerAddressTypeMaintenanceDocument-EditCustomerAddressType"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>customerAddressTypeCode</value>
      </list>
    </property>
    
    
    <property name="documentTypeName" value="CATY"/>
    <property name="workflowAttributes">
    	<ref bean="CustomerAddressTypeMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="CustomerAddressTypeMaintenanceDocument-EditCustomerAddressType" parent="CustomerAddressTypeMaintenanceDocument-EditCustomerAddressType-parentBean"/>

  <bean id="CustomerAddressTypeMaintenanceDocument-EditCustomerAddressType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Customer Address Type"/>
    <property name="title" value="Edit Customer Address Type"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="customerAddressTypeCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="customerAddressTypeDescription" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="active" p:defaultValue="true"/>
      </list>
    </property>
  </bean>
  
  <!-- Start - Workflow Attribute Beans -->
  <bean id="CustomerAddressTypeMaintenanceDocument-workflowAttributes" parent="CustomerAddressTypeMaintenanceDocument-workflowAttributes-parentBean"/>
	
  <bean id="CustomerAddressTypeMaintenanceDocument-workflowAttributes-parentBean" abstract="true" parent="WorkflowAttributes">   	
  </bean>
</beans>
