<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN" "http://directwebremoting.org/schema/dwr30.dtd">

<dwr>
	<init>
		<creator id="spring" class="org.kuali.rice.kns.web.servlet.dwr.GlobalResourceDelegatingSpringCreator"/>
	</init>
    <allow>
        <convert converter="bean" match="org.kuali.kfs.module.cg.businessobject.SubContractor">
            <param name="include" value="subcontractorName" />
        </convert>
        <create creator="spring" javascript="SubcontractorService">
            <param name="beanName" value="subcontractorService" />
            <include method="getByPrimaryId" />
        </create>
        <convert converter="bean" match="org.kuali.kfs.module.cg.businessobject.Agency">
            <param name="include" value="fullName,agencyTypeCode" />
        </convert>
        <create creator="spring" javascript="AgencyService">
            <param name="beanName" value="agencyService" />
            <include method="getByPrimaryId" />
        </create>	    
    </allow>
</dwr>

