  <bean id="UniversityDateMaintenanceDocument" parent="UniversityDateMaintenanceDocument-parentBean"/>

  <bean id="UniversityDateMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.sys.businessobject.UniversityDate"/>
    <property name="maintainableClass" value="org.kuali.kfs.sys.document.FinancialSystemMaintainable"/>
    <property name="maintainableSections">
      <list>
        <ref bean="UniversityDateMaintenanceDocument-EditUniversityDate"/>
      </list>
    </property>
    <property name="defaultExistenceChecks">
      <list>
        <bean parent="ReferenceDefinition" p:attributeName="accountingPeriod" p:attributeToHighlightOnFail="universityFiscalAccountingPeriod"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>universityDate</value>
      </list>
    </property>
    
    
    
    
    
    <property name="documentTypeName" value="UDAT"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
    	<ref bean="UniversityDateMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="UniversityDateMaintenanceDocument-EditUniversityDate" parent="UniversityDateMaintenanceDocument-EditUniversityDate-parentBean"/>

  <bean id="UniversityDateMaintenanceDocument-EditUniversityDate-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit University Date"/>
    <property name="title" value="Edit University Date"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="universityDate" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="universityFiscalYear" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="universityFiscalAccountingPeriod" p:required="true"/>
      </list>
    </property>
  </bean>
  
    <!-- workflow attributes -->
  <bean id="UniversityDateMaintenanceDocument-workflowAttributes" parent="UniversityDateMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="UniversityDateMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.krad.datadictionary.WorkflowAttributes" abstract="true"/>
</beans>
